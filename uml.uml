@startuml
' Define ExpenseApp Class
class ExpenseApp {
  - expenses: Array
  + addExpense(name: String, amount: Number, date: String): void
  + deleteExpense(id: Number): void
  + displayExpenses(): void
  + updateTotal(): void
}

' Define Expense Class
class Expense {
  - id: Number
  - name: String
  - amount: Number
  - date: String
}

' Relationships
ExpenseApp "1" *-- "many" Expense : manages

@enduml
